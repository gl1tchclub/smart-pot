generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Plant {
  id              Int         @id @default(autoincrement())
  name            String      @unique
  daysWater       Int
  threshold       Int
  temp            Int
  indoor          Boolean
  updatedAt       DateTime     @updatedAt
}